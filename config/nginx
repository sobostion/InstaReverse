server {
        listen 80 default_server;
        listen [::]:80 default_server;

                root /home/ubuntu/insta-reverse/app;

                index main.css;

        server_name _;

        location / {
                include  /etc/nginx/mime.types;
                proxy_pass http://127.0.0.1:5000;
        }
        location /static/css {
                include  /etc/nginx/mime.types;
        }
}
